{
    "/api/*": {
        "target": "http://localhost:9000",
        "secure": false,
        "logLevel": "debug",
        "changeOrigin": true
    },
    "/controller/api/*": {
        "target": "http://localhost:3000",
        "secure": false,
        "pathRewrite": {
            "^/controller/api/": "/api/controller/"
        },
        "logLevel": "debug",
        "changeOrigin": true
    }
}